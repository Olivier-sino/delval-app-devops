{
  "uid": "delvel-overview",
  "title": "DELVEL App Overview",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests per second",
      "id": 1,
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_count[1m])",
          "legendFormat": "rps"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Average latency (s)",
      "id": 2,
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[1m]) / rate(http_request_duration_seconds_count[1m])",
          "legendFormat": "avg"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "95th percentile latency (s)",
      "id": 3,
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Process memory (MB)",
      "id": 4,
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes / 1024 / 1024",
          "legendFormat": "rss"
        }
      ]
    }
  ]
}
